  "paths": {
    "/v1/auth/client": {
      "post": {
        "description": "Authenticate with service and retrieve an access_token required for other endpoints.", 
        "parameters": [
          {
            "in": "body", 
            "name": "body", 
            "required": false, 
            "schema": {
              "properties": {
                "id": {
                  "type": "string"
                }, 
                "secret": {
                  "type": "string"
                }
              }, 
              "type": "object"
            }
          }
        ], 
        "responses": {
          "default": {
            "description": "", 
            "schema": {
              "$ref": "#/definitions/AuthorizationResponse"
            }
          }
        }
      }
    }, 
    "/v1/mga": {
      "get": {
        "description": "List of available MGAs.", 
        "parameters": [
          {
            "description": "Header with access token retrieved from /v1/auth/client. e.g. Bearer xxxxxx.yyyyyyy.zzzzzz", 
            "in": "header", 
            "name": "Authorization", 
            "required": true, 
            "type": "string"
          }
        ], 
        "responses": {
          "default": {
            "description": "", 
            "schema": {
              "items": {
                "$ref": "#/definitions/MgaResponse"
              }, 
              "type": "array"
            }
          }
        }
      }
    }, 
    "/v1/mga/{mga_id}/report": {
      "get": {
        "description": "List available reports for given MGA.", 
        "parameters": [
          {
            "in": "path", 
            "name": "mga_id", 
            "required": true, 
            "type": "string"
          }, 
          {
            "description": "Header with access token retrieved from /v1/auth/client. e.g. Bearer xxxxxx.yyyyyyy.zzzzzz", 
            "in": "header", 
            "name": "Authorization", 
            "required": true, 
            "type": "string"
          }
