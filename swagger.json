            "required": true, 
            "type": "string"
          }, 
          {
            "in": "path", 
            "name": "entity", 
            "required": true, 
            "type": "string"
          }, 
          {
            "description": "Return a url to download the data in the given file type. Valid values are json, csv. Defaults to json", 
            "in": "query", 
            "name": "file_type", 
            "required": false, 
            "type": "string"
          }, 
          {
            "description": "Header with access token retrieved from /v1/auth/client. e.g. Bearer xxxxxx.yyyyyyy.zzzzzz", 
            "in": "header", 
            "name": "Authorization", 
            "required": true, 
            "type": "string"
          }
        ], 
        "responses": {
          "default": {
            "description": "", 
            "schema": {
              "$ref": "#/definitions/UrlResponse"
            }
          }
        }
      }
    }, 
