    "/v1/mga/{mga_id}/report/{report_id}/format/{format_type}/entities/{entity}": {
      "get": {
        "description": "Entity data of the report in the specified file type.", 
        "parameters": [
          {
            "in": "path", 
            "name": "report_id", 
            "required": true, 
            "type": "string"
          }, 
          {
            "in": "path", 
